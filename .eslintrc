{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  rules: {
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "keyword-spacing": ["error", {"overrides": {
      "if": {"after": false}
    }}],
    "object-curly-spacing": 0,
    "no-underscore-dangle": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0
  }
}
